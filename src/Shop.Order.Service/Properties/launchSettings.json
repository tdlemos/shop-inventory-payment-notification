{
  "profiles": {
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5291"
    },
    "https": {
      "applicationUrl": "https://localhost:7005;http://localhost:5291",
      "commandName": "Project",
      "dotnetRunMessages": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        //"DefaultConnection": "Server=localhost;Port=5432;Database=orders;User Id=ordersUser;Password=admin;"
        "DefaultConnection": "Host=localhost;Database=orders;Username=ordersUser;Password=admin"
        //"DefaultConnection": "Server=localhost;Port=5432;Initial Catalog=orders;User Id=ordersUser;Password=admin;"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}